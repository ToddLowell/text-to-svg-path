{
  "env": {
    "browser": true,
    "commonjs": true,
    "jest/globals": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended"
    // "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [
    "jest"
    // "@typescript-eslint"
  ],
  // "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/no-var-requires": 0
  }
}
